{
    "name": "flarumite/flarum-docker",
    "description": "Simple Docker image to aid Flarum extension development.",
    "type": "project",
    "keywords": [
        "flarum",
        "docker"
    ],
    "homepage": "https://flarumite.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Ian Morland",
            "email": "ian@flarumite.com"
        }
    ],
    "support": {
        "issues": "https://github.com/flarumite/flarum-docker/issues",
        "source": "https://github.com/flarumite/flarum-docker",
        "docs": "https://flarum.org/docs/"
    },
    "require": {
        "php": ">=7.4",
        "bokt/flarum-cache-assets": "^0.2.0",
        "clarkwinkelmann/flarum-ext-circle-groups": "^0.2.1",
        "flarum/approval": "^0.1.0",
        "flarum/bbcode": "^0.1.0",
        "flarum/core": "^0.1.0",
        "flarum/emoji": "^0.1.0",
        "flarum/flags": "^0.1.0",
        "flarum/lang-english": "^0.1.0",
        "flarum/likes": "^0.1.0",
        "flarum/lock": "^0.1.0",
        "flarum/markdown": "^0.1.0",
        "flarum/mentions": "^0.1.0",
        "flarum/statistics": "^0.1.0",
        "flarum/sticky": "^0.1.0",
        "flarum/subscriptions": "^0.1.0",
        "flarum/suspend": "^0.1.0",
        "flarum/tags": "^0.1.0",
        "flarumite/flarum-decontaminator": "^0.1.1",
        "fof/best-answer": "^0.1.7",
        "fof/byobu": "^0.5.0",
        "fof/console": "^0.5.3",
        "fof/default-user-preferences": "^0.2.0",
        "fof/follow-tags": "^0.3.4",
        "fof/formatting": "^0.1.4",
        "fof/ignore-users": "^0.1.6",
        "fof/impersonate": "^0.4.1",
        "fof/links": "^0.3.0",
        "fof/merge-discussions": "^0.3.3",
        "fof/moderator-notes": "^0.1.3",
        "fof/profile-image-crop": "^0.1.1",
        "fof/reactions": "^0.3.1",
        "fof/sitemap": "^0.3.0",
        "fof/split": "^0.4.3",
        "fof/upload": "^0.9.1",
        "fof/user-bio": "^0.1.3",
        "franzl/flarum-open-links-in-new-tab": "^0.1.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "workbench/*"
        }
    ]
}
